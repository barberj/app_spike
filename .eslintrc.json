{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "modules": true,
      "arrowFunctions": true,
      "classes": true
    },
    "env": {
        "jest": true,
        "es6": true,
        "browser": true
    },
    "rules": {
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "quotes": [2, "double"]
    }
}
